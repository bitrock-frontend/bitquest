{
  "name": "bitquest",
  "version": "1.0.0",
  "description": "Makes JSON requests with fetch easier, the Bitrock way!",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "xo && ava test/index.js --verbose",
    "compile": "tsc ./src/libs.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitrock-frontend/bitquest.git"
  },
  "keywords": [
    "fetch",
    "API",
    "wrapper",
    "JSON",
    "REST",
    "bitrock"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "contributors": [
    "Salvatore Laisa <salvatore.laisa@bitrock.it>"
  ],
  "license": "MIT",
  "xo": {
    "env": [
      "browser",
      "node"
    ],
    "space": true,
    "rules": {
      "ava/prefer-async-await": 0,
      "no-unused-expressions": 0,
      "max-params": 0,
      "no-use-before-define": 0
    }
  },
  "bugs": {
    "url": "https://github.com/bitrock-frontend/bitquest/issues"
  },
  "homepage": "https://github.com/bitrock-frontend/bitquest",
  "devDependencies": {
    "ava": "0.25.0",
    "node-fetch": "2.1.2",
    "typescript": "2.8.1",
    "xo": "0.20.3"
  }
}
